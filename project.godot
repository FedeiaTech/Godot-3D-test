config_version=5

[animation]

warnings/check_angle_interpolation_type_conflicting=false

[application]

config/name="astronaut 0.0.4"
config/tags=PackedStringArray("2d", "openworld", "uptodown")
run/main_scene="res://Scenes/Menu.tscn"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=60
boot_splash/show_image=false

[autoload]

Global="*res://Scripts/Global/global.gd"
GlobalItemList="*res://Scripts/Global/global_item_list.gd"
scene_manager="*res://Scripts/scene_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
GameManager="*res://Scenes/GameManager/game_manager.tscn"

[display]

window/size/viewport_width=1140
window/size/viewport_height=750
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Assets/Cursor/hand_astronaut3.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

